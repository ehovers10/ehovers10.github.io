@Unpublished{abbott,
  Title                    = {Presuppositions, Negation, and Existence},
  Author                   = {Barbara Abbott},
  Abstract                 = {Last year (2005) marked the 100th anniversary of the publication of Russell\textquoteright{}s classic \textquoteleft{}On denoting\textquoteright. It should not cast any shadow on that great work to note that the problems it provided solutions to are still the subject of controversy. Two of those problems involved noun phrases (NPs) which fail to denote. Russell\textquoteright{}s examples (1a) and (1b) (1) a. The king of France is bald. b. The king of France is not bald. are puzzling because they have the form of simple contradictories, and yet we are not inclined to say either one is true. Example (2) (2) Pegasus does not exist. is even more problematic; the lack of denotation for Pegasus, which makes the sentence true, also seems to rob it of a meaningful constituent. Once the king of France is unpacked according to Russell\textquoteright{}s analysis, (1b) is revealed to be ambiguous. It\textquoteright{}s logical forms are given in (3). (3) a. $\exists${}x[Kx $\wedge$ $\forall${}y[Ky $\leftrightarrow$ y=x] $\wedge$ \textlnot{}Bx] b. \textlnot$\exists${}x[Kx $\wedge$ $\forall${}y[Ky $\leftrightarrow$ y=x] $\wedge$ Bx] (3a) says that there is a unique (French) king who is not bald (obviously false), but (3b), the logical contradictory of (1a) says that it is not the case that there is a unique king who is bald (which is true). We can apply the analysis to sentence (2) once we recognize Pegasus as a concealed definite description, e.g. the winged horse of Greek mythology. (2) can then be unpacked as (4) (4) \textlnot$\exists${}x[Wx $\wedge$ $\forall${}y[Wy $\leftrightarrow$ y=x]] which seems both meaningful and true, as required. Problems solved. Well, not quite. Strawson (1950) challenged the first solution above, arguing that neither (1a) nor (1b) could be used to assert the existence of a king of France. Rather, use of such sentences presupposes the existence of a king of France, and failing that existence, neither of (1a) or (1b) could be used to make either a true or a false statement -- in Strawson\textquoteright{}s words, \textquotedblleft{}the question of whether it\textquoteright{}s true or false simply doesn\textquoteright{}t arise\textquotedblright (Strawson 1950, 330).1 In an extended series of essays, and one book, Jay Atlas (1977, 1978, 1979, 1989, 2004) has taken issue with the work of both Russell and Strawson..},
  Owner                    = {erik},
  Timestamp                = {2014.10.05}
}

@Unpublished{abbotta,
  Title                    = {Where Have Some of the Presuppositions Gone?},
  Author                   = {Barbara Abbott},
  Abstract                 = {Some presuppositions seem to be weaker than others in the sense that they can be more easily neutralized in some contexts. For example some factive verbs, most notably epistemic factives like know, be aware, and discover, are known to shed their factivity fairly easily in contexts such as are found in (1). (1) a. \textellipsis{}if anyone discovers that the method is also wombat-proof, I\textquoteright{}d really like to know! b. Mrs. London is not aware that there have ever been signs erected to stop use of the route\textellipsis c. Perhaps God knows that we will never reach the stars\textellipsis. (The examples in (1) are all naturally occurring ones, discovered by David Beaver with the aid of Google; cf. Beaver 2002, exx. 32, 43, and 51, respectively.) On the other hand some other factives, e.g. regret, matter, and be surprised, do not exhibit the same type of behavior: (2) a. If any of the students regrets behaving badly, they\textquoteright{}ll let us know. b. It doesn\textquoteright{}t matter that the chimpanzees escaped. c. Was Bill surprised that spinach was included? Unlike the examples in (1), those in (2) could not be used appropriately in contexts where the speaker was not assuming that the complement clause was true. Our main concern will be trying to find the cause of this difference. However, before we get to that, we will look more closely at the concept \textquotedblleft{}presupposition\textquotedblright itself, as well as its close neighbor in the linguistic literature, \textquotedblleft{}conventional implicature\textquotedblright (section 2), and also at various ways of getting rid of presuppositions (section 3). In section 4 we will investigate two possible explanations for differences in presupposition triggering -- the \textquotedblleft{}lexical alternative\textquotedblright approach of Abusch (2002, 2005), and a suggestion of Ladusaw\textquoteright{}s involving detachability of presuppositions. The final section contains concluding remarks.},
  Owner                    = {erik},
  Timestamp                = {2014.10.05}
}

@Article{abbott2008,
  Title                    = {Presuppositions and Common Ground},
  Author                   = {Barbara Abbott},
  Journal                  = {Linguistics and Philosophy},
  Year                     = {2008},
  Number                   = {5},
  Pages                    = {523--538},
  Volume                   = {31},
  Abstract                 = {This paper presents problems for Stalnaker\textquoteright{}s common ground theory of presupposition. Stalnaker (Linguist and Philos 25:701--{}721, 2002) proposes a 2-stage process of utterance interpretation: presupposed content is added to the common ground prior to acceptance/rejection of the utterance as a whole. But this revision makes presupposition difficult to distinguish from assertion. A more fundamental problem is that the common ground theory rests on a faulty theory of assertion---{}that the essence of assertion is to present the content of an utterance as new information. Many examples are presented of utterances which are felicitous but not informative in this way.},
  Owner                    = {erik},
  Timestamp                = {2014.10.05}
}

