@Article{Edwards2002,
  Title                    = {Theories of Meaning and Logical Constants: Davidson versus Evans},
  Author                   = {Edwards, Jim},
  Journal                  = {Mind},
  Year                     = {2002},
  Number                   = {442},
  Pages                    = {pp. 249-279},
  Volume                   = {111},

  Abstract                 = {Donald Davidson has claimed that a theory of meaning identifies the logical constants of the object language by treating them in the phrasal axioms of the theory, and that the theory entails a relation of logical consequence among the sentences of the object language. Section 1 offers a preliminary investigation of these claims. In Section 2 the claims are rebutted by appealing to Evans's paradigm of a theory of meaning. Evans's theory is deliberately blind to any relation of logical consequence among the sentences of the object language, and entails only what Evans takes to be a distinct and deeper relation of structural validity among the sentences of the object language. In Section 3 we turn to Evans's motivation in order to compare the two paradigms of a theory of meaning. Evans laid down criteria under which a theory of meaning gives what he called a 'transcendent' semantic classification of the lexicon of the object language, in contrast to a mere 'immanent' classification. However, when these criteria are applied we find that, pace Evans, they favour Davidson's paradigm over Evans's. In the final section we show that Evans's conception of structural consequence turns out to be a deeper formulation of logical consequence.},
  Copyright                = {Copyright © 2002 Oxford University Press},
  ISSN                     = {00264423},
  Jstor_articletype        = {research-article},
  Jstor_formatteddate      = {Apr., 2002},
  Language                 = {English},
  Owner                    = {erik},
  Publisher                = {Oxford University Press on behalf of the Mind Association},
  Series                   = {New Series},
  Timestamp                = {2011.11.22},
  Url                      = {http://www.jstor.org/stable/3093708}
}

@InCollection{egan2010,
  Title                    = {Disputing about taste},
  Author                   = {Andy Egan},
  Booktitle                = {Disagreement},
  Publisher                = {Oxford University Press},
  Year                     = {2010},
  Editor                   = {Ted Warfield and Richard Feldman},

  Abstract                 = {i$>$ \textquotedblleft{}There\textquoteright{}s no disputing about taste.\textquotedblright That\textquoteright{}s got a nice ring to it, but it\textquoteright{}s not quite the ring of truth. While there\textquoteright{}s definitely something right about the aphorism -- there\textquoteright{}s a reason why it is, after all, an aphorism, and why its utterance tends to produce so much nodding of heads and muttering of \textquotedblleft{}just so\textquotedblright and \textquotedblleft{}yes, quite\textquotedblright -- it\textquoteright{}s surprisingly difficult to put one\textquoteright{}s finger on just what the truth in the neighborhood is, exactly. One thing that\textquoteright{}s pretty clear is that what\textquoteright{}s right about the aphorism, that there\textquoteright{}s no disputing about taste, isn\textquoteright{}t that there\textquoteright{}s no disputing about taste. There\textquoteright{}s heaps of disputing about taste. People engage in disputes about which movies, music, paintings, literature, meals, furniture, architectural styles, etc. are good, beautiful, tasty, fun, elegant, ugly, disgusting, etc. all the time. This is obvious to anyone who has watched dueling-movie-critics shows, read theater reviews, or negotiated with a group or partner about which movie or restaurant to go to, or which sofa or painting to put in the living room. It takes great care and good aim to fling a brick without hitting somebody who\textquoteright{}s engaged in a dispute about taste.},
  File                     = {egan2010.pdf:egan2010.pdf:PDF},
  Owner                    = {erik},
  Timestamp                = {2011.11.22},
  Url                      = {http://philpapers.org/go.pl?id=BIRDGN&u=http%3A%2F%2Fweb%2Eme%2Ecom%2Feganam%2FAndy_Egan%2FPapers_files%2Fde%2520gustibus%2E2010%2E02%2E27%2Epdf}
}

