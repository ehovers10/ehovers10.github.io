@Other{bartgeurts,
  Title                    = {doi: 10.3765/sp.2.5 Free choice for all: a response to Emmanuel Chemla ∗},
  __markedentry            = {[erik:]},
  Abstract                 = {Abstract Chemla (2009) presents experimental data purporting to show that speakers ’ intuitions about so-called “embedded implicatures ” cause trouble for globalist and localist theories alike. We explain, to begin with, that the way Chemla frames the debate between localists and globalists fails to do justice to the latter. Then we turn to his experimental data, and argue that, while of half of them strengthen our own case against localism (Geurts &amp; Pouscoulous 2009), the other half do not jeopardize the globalist view, as Chemla claims they do.},
  Author                   = {Bart Geurts, Nausicaa Pouscoulous},
